<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="com.scrop.hall.selnumsfragment.SelNumsSSCBhFragment">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <com.scrop.view.selnums.SelNumsStyle0Layout
                android:id="@+id/id_sns_bh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:hiddenOmit="true"
                app:digit="选号">
            </com.scrop.view.selnums.SelNumsStyle0Layout>
            <com.scrop.view.selnums.SelNumsFilter
                android:id="@+id/id_snf_bh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            </com.scrop.view.selnums.SelNumsFilter>
        </LinearLayout>
    </ScrollView>

</FrameLayout>

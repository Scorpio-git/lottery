<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="com.scrop.hall.selnumsfragment.SelNumsSSCNormalFragment">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <com.scrop.view.selnums.SelNumsStyle0Layout
                android:id="@+id/id_sns_w"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:digit="万位">
            </com.scrop.view.selnums.SelNumsStyle0Layout>
            <com.scrop.view.selnums.SelNumsStyle0Layout
                android:id="@+id/id_sns_q"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:digit="千位">
            </com.scrop.view.selnums.SelNumsStyle0Layout>
            <com.scrop.view.selnums.SelNumsStyle0Layout
                android:id="@+id/id_sns_b"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:digit="百位">
            </com.scrop.view.selnums.SelNumsStyle0Layout>
            <com.scrop.view.selnums.SelNumsStyle0Layout
                android:id="@+id/id_sns_s"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:digit="十位">
            </com.scrop.view.selnums.SelNumsStyle0Layout>
            <com.scrop.view.selnums.SelNumsStyle0Layout
                android:id="@+id/id_sns_g"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:digit="个位">
            </com.scrop.view.selnums.SelNumsStyle0Layout>
            <View
                android:layout_width="match_parent"
                android:layout_height="80dp"/>
        </LinearLayout>

    </ScrollView>

</FrameLayout>
